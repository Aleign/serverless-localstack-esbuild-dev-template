    "docker-up": "./scripts/docker-compose.sh up",
    "docker-run": "./scripts/docker-compose.sh run",
    "start-localstack": "yarn docker-up -d localstack",
    "start-localstack-debugmode": "LAMBDA_DEBUG_MODE=1 LAMBDA_LOG_LEVEL=debug DEBUG=1 yarn docker-up -d localstack",
    "log-localstack": "./scripts/docker-logs.sh localstack-main",
    "log-lambda": "./scripts/docker-logs.sh --filter localstack-main-lambda",
    "kill-localstack": "./scripts/docker-stop.sh localstack-main",
    "kill-lambda": "./scripts/docker-stop.sh localstack-main-lambda",
    "test": "jest --verbose=true --silent=false",
    "test:watch": "jest --watch",
    "esbuild-metafile": "node ./scripts/es-metafile.js",
    "node-arch": "echo \"Node Architecure: $(node -p \"process.arch\")\"",
    "clean-serverless-package": "./scripts/yarnclean-serverless.sh",
    "setenvvars": "./scripts/set-env-vars.sh"
