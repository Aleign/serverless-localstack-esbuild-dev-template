  "scripts": {
    "stop": "./scripts/stop-localstack.sh",
    "start": "LOCALSTACK_MAIN_DEBUG=1 ./scripts/start.sh",
    "debug": "./scripts/start.sh --debugmode",
    "frozen": "yarn install --frozen-lockfile",
    "watch": "node scripts/watch.js",
    "build": "node scripts/build.js",
    "destroy": "./scripts/destroy.sh",
    "clean": "./scripts/clean.sh",
    "deploy-dev": "CUSTOM_ID=aleign ./scripts/serverless.sh deploy --debug",
    "package-dev": "CUSTOM_ID=aleign ./scripts/serverless.sh package -debug",
